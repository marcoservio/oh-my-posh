{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "#d1d1d1",
                    "properties": {
                        "always_enabled": true,
                        "style": "round"
                    },
                    "style": "plain",
                    "template": " {{ .FormattedMs }} <#d1d1d1,>|</>",
                    "type": "executiontime"
                },
                {
                    "foreground": "green",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}red{{ end }}"
                    ],
                    "properties": {
                        "always_enabled": true
                    },
                    "style": "plain",
                    "template": " {{ if gt .Code 0 }}\u2a2f{{else}}\u2713{{ end }} <#d1d1d1,>|</>",
                    "type": "exit"
                },
                {
                    "type": "sysinfo",
                    "style": "plain",
                    "powerline_symbol": "",
                    "foreground": "#f1fa8c",
                    "template": "  {{ round .PhysicalPercentUsed .Precision }}% <#d1d1d1,>|</> ",
                    "properties": {
                        "precision": 2
                    }
                },
                {
                    "type": "dotnet",
                    "style": "plain",
                    "powerline_symbol": "",
                    "foreground": "#9f00c7",
                    "template": " {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} <#d1d1d1,>|</> "
                },
                {
                    "foreground": "#ff5555",
                    "style": "plain",
                    "template": "{{ .Name }}",
                    "type": "shell"
                },
                {
                    "foreground": "#d1d1d1",
                    "properties": {
                        "time_format": "15:04:05"
                    },
                    "style": "plain",
                    "template": " <#d1d1d1,>|</> {{ .CurrentDate | date .Format }}",
                    "type": "time"
                },
                {
                    "type": "battery",
                    "style": "plain",
                    "foreground": "#50fa7b",
                    "template": " <#d1d1d1,>|</> {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}",
                    "properties": {
                        "discharging_icon": " ",
                        "charging_icon": " ",
                        "charged_icon": " "
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#d1d1d1",
                    "style": "plain",
                    "template": "{{.Icon}} ",
                    "type": "os"
                },
                {
                    "foreground": "#d1d1d1",
                    "style": "plain",
                    "template": "(<#50fa7b>{{ .UserName }} </><#d1d1d1>\u26A1</><#50fa7b>{{ .HostName }}</>)",
                    "type": "session"
                },
                {
                    "foreground": "#d1d1d1",
                    "properties": {
                        "folder_separator_icon": "/",
                        "style": "full"
                    },
                    "style": "plain",
                    "template": " <#d1d1d1></> [<#bd93f9>{{ .Path }}</>] ",
                    "type": "path"
                },
                {
                    "foreground": "#ff79c6",
                    "powerline_symbol": "",
                    "properties": {
                        "fetch_stash_count": true,
                        "fetch_upstream_icon": true
                    },
                    "style": "plain",
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }}",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#cd5e42",
                    "style": "plain",
                    "template": " ",
                    "type": "root"
                },
                {
                    "foreground": "#CD4277",
                    "style": "plain",
                    "template": "<#66CDAA>\ue285</><#26C6DA>\ue285</><#26da5c>\ue285</> ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 2
}
